
<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
    </tr>
    <% obj.each do |article|%>
    <tr>
        <td><%= article.title%></td>
        <td><%= article.description%></td>
        
       
        <td><%= link_to 'show',article_path(article)%></td>
         <% if logged_in? && article.user == current_user%>
            <td><%= link_to 'edit',edit_article_path(article)%></td>
            
            <td><%= link_to 'destroy',article_path(article),method: :delete,data: {confirm: "Are you suere"}%></td>
    <% end %>
    </tr>
    <% end %>
</table>